{
	"name": "@x-captcha/server",
	"version": "1.0.0",
	"description": "Server implementation for X-Captcha service",
	"keywords": [
		"captcha",
		"security",
		"server"
	],
	"license": "MIT",
	"author": "",
	"main": "dist/main.js",
	"scripts": {
		"build": "nest build",
		"clean": "rimraf dist",
		"commit": "cz",
		"debug": "nest start --debug --watch",
		"dev": "nest start --watch",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint ./",
		"lint:all": "npm run lint && npm run lint:types",
		"lint:all:fix": "npm run lint:fix && npm run lint:types:fix",
		"lint:fix": "eslint --fix ./",
		"lint:types": "tsc --noEmit",
		"lint:types:fix": "tsc --noEmit --skipLibCheck",
		"lint:watch": "npx eslint-watch ./",
		"migration:generate": "npm run typeorm -- migration:generate -d ./src/data-source.ts",
		"migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
		"migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
		"prepare": "husky",
		"release": "semantic-release",
		"start": "node dist/main.js",
		"start:debug": "nest start --debug --watch",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main.js",
		"test": "jest",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json",
		"test:watch": "jest --watch",
		"typecheck": "tsc --noEmit",
		"typeorm": "ts-node ./node_modules/typeorm/cli"
	},
	"config": {
		"commitizen": {
			"path": "@elsikora/commitizen-plugin-commitlint-ai"
		}
	},
	"dependencies": {
		"@aws-sdk/client-cloudwatch-logs": "^3.772.0",
		"@elsikora/nestjs-aws-parameter-store-config": "^1.2.14",
		"@elsikora/nestjs-crud-automator": "^1.7.0",
		"@elsikora/nestjs-typeorm-aws-connector": "^1.3.1",
		"@fastify/static": "^8.1.1",
		"@nestjs/common": "^11.0.12",
		"@nestjs/config": "^4.0.1",
		"@nestjs/core": "^11.0.12",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-fastify": "^11.0.12",
		"@nestjs/swagger": "^11.0.7",
		"@nestjs/throttler": "^6.4.0",
		"@nestjs/typeorm": "^11.0.0",
		"@sentry/node": "^9.8.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"cors": "^2.8.5",
		"fastify": "^5.2.1",
		"passport-custom": "^1.1.1",
		"pg": "^8.14.1",
		"reflect-metadata": "^0.2.2",
		"request-ip": "^3.3.0",
		"rxjs": "^7.8.2",
		"typeorm": "^0.3.21",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@elsikora/commitizen-plugin-commitlint-ai": "^1.0.0",
		"@elsikora/eslint-config": "^3.7.12",
		"@elsikora/eslint-plugin-nestjs-typed": "^3.0.1",
		"@nestjs/cli": "^11.0.5",
		"@nestjs/testing": "^11.0.12",
		"@saithodev/semantic-release-backmerge": "^4.0.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.1",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/request-ip": "^0.0.41",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"commitizen": "^4.3.1",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"eslint": "^9.23.0",
		"eslint-plugin-n": "^17.16.2",
		"eslint-plugin-ng-module-sort": "^1.3.1",
		"eslint-plugin-no-secrets": "^2.2.1",
		"eslint-plugin-typeorm-typescript": "^0.5.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.5.0",
		"prettier": "^3.5.3",
		"rimraf": "^5.0.5",
		"semantic-release": "^24.2.3",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3"
	}
}
