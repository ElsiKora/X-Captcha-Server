{
	"name": "@x-captcha/server",
	"version": "1.0.0",
	"description": "Server implementation for X-Captcha service",
	"keywords": [
		"captcha",
		"security",
		"server"
	],
	"license": "MIT",
	"author": "",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc",
		"clean": "rimraf dist",
		"commit": "cz",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "eslint ./",
		"lint:all": "npm run lint && npm run lint:types",
		"lint:all:fix": "npm run lint:fix && npm run lint:types:fix",
		"lint:fix": "eslint --fix ./",
		"lint:types": "tsc --noEmit",
		"lint:types:fix": "tsc --noEmit --skipLibCheck",
		"lint:watch": "npx eslint-watch ./",
		"prepare": "husky",
		"release": "semantic-release",
		"start": "node dist/index.js",
		"typecheck": "tsc --noEmit"
	},
	"config": {
		"commitizen": {
			"path": "@elsikora/commitizen-plugin-commitlint-ai"
		}
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.8.0",
		"@commitlint/config-conventional": "^19.8.0",
		"@elsikora/commitizen-plugin-commitlint-ai": "^1.0.0",
		"@elsikora/eslint-config": "^3.7.11",
		"@elsikora/eslint-plugin-nestjs-typed": "^3.0.1",
		"@saithodev/semantic-release-backmerge": "^4.0.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^11.0.1",
		"@semantic-release/npm": "^12.0.1",
		"@semantic-release/release-notes-generator": "^14.0.3",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"commitizen": "^4.3.1",
		"conventional-changelog-conventionalcommits": "^8.0.0",
		"eslint": "^9.23.0",
		"eslint-plugin-n": "^17.16.2",
		"eslint-plugin-ng-module-sort": "^1.3.1",
		"eslint-plugin-no-secrets": "^2.2.1",
		"eslint-plugin-typeorm-typescript": "^0.5.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^15.5.0",
		"prettier": "^3.5.3",
		"rimraf": "^5.0.5",
		"semantic-release": "^24.2.3",
		"supertest": "^6.3.4",
		"ts-jest": "^29.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3"
	}
}
